#import os
#os.environ['SFPENOPTS'] = 'bgcolor=w'

from rsf.proj import *
 
#
# Make d.rsf
#
Flow('data1 my_tfile',None,
     '''
     ~/rsf/src/user/astanton/sfsyn5d
     tfile=${TARGETS[1]} 
     n1=1000 n2=100 n3=100 n4=1 n5=1 
     d1=0.001 d2=10 d3=10 d4=10 d5=10 
     o2=250 o3=250 o4=500 o5=500 
     nevent=4 amp=1,-1,1,-1 t0=0.2,0.4,0.6,0.8 f0=10,20,30,40 vx=1500,1500,1500,1500 vy=1200,1200,1200,1200
     title="1 receiver gather"
     ''') 

Result('data1',
       '''
       window min4=500 n4=1 min5=500 n5=1 |
       byte gainpanel=all |
       grey3 frame1=650 frame2=25 frame3=25
       screenratio=1 
       ''')

End()

